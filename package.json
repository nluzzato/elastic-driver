{
  "name": "alert-context-agent",
  "version": "1.0.0",
  "description": "AI agent that provides context for Prometheus alerts by reading alert definitions from GitHub",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "simple": "ts-node src/simple-demo.ts",
    "alert": "ts-node src/run-alert.ts",
    "quick": "ts-node src/run-simple.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "server": "ts-node src/server.ts",
    "gui:dev": "concurrently -k -n SERVER,UI -c yellow,cyan \"ts-node src/server.ts\" \"vite\"",
    "gui:build": "vite build",
    "gui:preview": "vite preview"
  },
  "keywords": [
    "prometheus",
    "alerts",
    "observability",
    "slack",
    "github",
    "devops"
  ],
  "author": "Connecteam",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.19.2",
    "openai": "^5.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
